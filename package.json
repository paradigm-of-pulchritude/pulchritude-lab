{
  "name": "pulchritude-lab",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "_apps/*",
    "_packages/*",
    "_config/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "kill:ports": "kill -9 $(lsof -ti:3000,3001,3002,3003,6010)",
    "remove:nm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "npm@8.1.2"
}
