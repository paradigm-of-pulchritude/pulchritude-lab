{
  "name": "pulchritude-lab",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "_apps/*",
    "_packages/*"
  ],
  "scripts": {
    "kill": "kill -9 $(lsof -ti:3000,3001,3002)",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "latest",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "npm@8.1.2"
}
